d$pred_label <- ifelse(d$pred_female_softmax < d$pred_male_softmax, 1, 0)
d$pos_neg <- ifelse(d$Labels==0 & d$pred_label==0, "true_negative",
ifelse(d$Labels==1 & d$pred_label==0, "false_negative",
ifelse(d$Labels==0 & d$pred_label==1, "false_positive", "true_positive")))
d$pred_label <- factor(d$pred_label)
d$Labels <- factor(d$Labels)
#Creating confusion matrix
example <- confusionMatrix(data=d$pred_label, reference = d$Labels)
#Display results
example
plt <- as.data.frame(example$table)
plt
plt$Prediction <- factor(plt$Prediction, levels=rev(levels(plt$Prediction)))
ggplot(plt, aes(Prediction,Reference, fill= Freq)) +
geom_tile() + geom_text(aes(label=Freq)) +
scale_fill_gradient(low="white", high="#009194") +
labs(x = "Ground truth",y = "Prediction") +
scale_x_discrete(position = "top", labels=c("male","female","female","male")) +
#scale_x_discrete(labels=c("male","female","female","male"), position = "top")
scale_y_discrete(labels=c("female","male","female","female"))
